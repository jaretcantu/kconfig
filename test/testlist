# Quick guide
#
# y|n A op B
#
# y|n - expected result of test
# op - arithmetic comparison operator (<. >, <=, >=)
# A/B - values to compare
#  %{...} - non-constant value
#   %{a~b} - Random value between a and b (inclusive)
#   %{^} - The value of the previous %{...} substitution

## Single digit tests
y %{0~8} <  %{^+1~9}
y %{0~8} <= %{^+1~9}
n %{0~8} >  %{^+1~9}
n %{0~8} >= %{^+1~9}
n %{1~9} <  %{0~^-1}
n %{1~9} <= %{0~^-1}
y %{1~9} >  %{0~^-1}
y %{1~9} >= %{0~^-1}
n %{0~9} <  %{^}
y %{0~9} <= %{^}
n %{0~9} >  %{^}
y %{0~9} >= %{^}

## Double digit tests -- first digits differ
y %{1~8}%{0~9} <  %{^+1~9}%{0~9}
y %{1~8}%{0~9} <= %{^+1~9}%{0~9}
n %{1~8}%{0~9} >  %{^+1~9}%{0~9}
n %{1~8}%{0~9} >= %{^+1~9}%{0~9}
n %{2~9}%{0~9} <  %{1~^-1}%{0~9}
n %{2~9}%{0~9} <= %{1~^-1}%{0~9}
y %{2~9}%{0~9} >  %{1~^-1}%{0~9}
y %{2~9}%{0~9} >= %{1~^-1}%{0~9}

## Double digit tests -- first digits the same
y %{1~9}%{1~8} <  %{^}%{^+1~9}
y %{1~9}%{1~8} <= %{^}%{^+1~9}
n %{1~9}%{1~8} >  %{^}%{^+1~9}
n %{1~9}%{1~8} >= %{^}%{^+1~9}
n %{1~9}%{2~9} <  %{^}%{1~^-1}
n %{1~9}%{2~9} <= %{^}%{1~^-1}
y %{1~9}%{2~9} >  %{^}%{1~^-1}
y %{1~9}%{2~9} >= %{^}%{1~^-1}

## Double digit equality
n %{1~9}%{0~9} <  %{^}%{^}
y %{1~9}%{0~9} <= %{^}%{^}
n %{1~9}%{0~9} >  %{^}%{^}
y %{1~9}%{0~9} >= %{^}%{^}

## Double/single digit tests -- opposite string sort order
n %{1~8}%{0~9} <  %{^+1~9}
n %{1~8}%{0~9} <= %{^+1~9}
y %{1~8}%{0~9} >  %{^+1~9}
y %{1~8}%{0~9} >= %{^+1~9}
n %{2~9}%{0~9} <  %{1~^-1}
n %{2~9}%{0~9} <= %{1~^-1}
y %{2~9}%{0~9} >  %{1~^-1}
y %{2~9}%{0~9} >= %{1~^-1}
y %{1~8} <  %{^+1~9}%{0~9}
y %{1~8} <= %{^+1~9}%{0~9}
n %{1~8} >  %{^+1~9}%{0~9}
n %{1~8} >= %{^+1~9}%{0~9}
y %{2~9} <  %{1~^-1}%{0~9}
y %{2~9} <= %{1~^-1}%{0~9}
n %{2~9} >  %{1~^-1}%{0~9}
n %{2~9} >= %{1~^-1}%{0~9}

## Double/single digit tests -- most significant zero
n 0%{1~9} <  %{0~^-1}
n 0%{1~9} <= %{0~^-1}
y 0%{1~9} >  %{0~^-1}
y 0%{1~9} >= %{0~^-1}
y 0%{0~8} <  %{^+1~9}
y 0%{0~8} <= %{^+1~9}
n 0%{0~8} >  %{^+1~9}
n 0%{0~8} >= %{^+1~9}
n 0%{0~9} <  %{^}
y 0%{0~9} <= %{^}
n 0%{0~9} >  %{^}
y 0%{0~9} >= %{^}
n %{1~9} <  0%{0~^-1}
n %{1~9} <= 0%{0~^-1}
y %{1~9} >  0%{0~^-1}
y %{1~9} >= 0%{0~^-1}
y %{0~8} <  0%{^+1~9}
y %{0~8} <= 0%{^+1~9}
n %{0~8} >  0%{^+1~9}
n %{0~8} >= 0%{^+1~9}
n %{0~9} <  0%{^}
y %{0~9} <= 0%{^}
n %{0~9} >  0%{^}
y %{0~9} >= 0%{^}

## Two version levels: test the major version differences
## Double digit tests -- first digits differ
y %{1~8}%{0~9}.%{0~9}%{0~9} <  %{^+1~9}%{0~9}.%{0~9}%{0~9}
y %{1~8}%{0~9}.%{0~9}%{0~9} <= %{^+1~9}%{0~9}.%{0~9}%{0~9}
n %{1~8}%{0~9}.%{0~9}%{0~9} >  %{^+1~9}%{0~9}.%{0~9}%{0~9}
n %{1~8}%{0~9}.%{0~9}%{0~9} >= %{^+1~9}%{0~9}.%{0~9}%{0~9}
n %{2~9}%{0~9}.%{0~9}%{0~9} <  %{1~^-1}%{0~9}.%{0~9}%{0~9}
n %{2~9}%{0~9}.%{0~9}%{0~9} <= %{1~^-1}%{0~9}.%{0~9}%{0~9}
y %{2~9}%{0~9}.%{0~9}%{0~9} >  %{1~^-1}%{0~9}.%{0~9}%{0~9}
y %{2~9}%{0~9}.%{0~9}%{0~9} >= %{1~^-1}%{0~9}.%{0~9}%{0~9}

## Double digit tests -- first digits the same
y %{1~9}%{1~8}-%{0~9}%{0~9} <  %{^}%{^+1~9}-%{0~9}%{0~9}
y %{1~9}%{1~8}-%{0~9}%{0~9} <= %{^}%{^+1~9}-%{0~9}%{0~9}
n %{1~9}%{1~8}-%{0~9}%{0~9} >  %{^}%{^+1~9}-%{0~9}%{0~9}
n %{1~9}%{1~8}-%{0~9}%{0~9} >= %{^}%{^+1~9}-%{0~9}%{0~9}
n %{1~9}%{2~9}-%{0~9}%{0~9} <  %{^}%{1~^-1}-%{0~9}%{0~9}
n %{1~9}%{2~9}-%{0~9}%{0~9} <= %{^}%{1~^-1}-%{0~9}%{0~9}
y %{1~9}%{2~9}-%{0~9}%{0~9} >  %{^}%{1~^-1}-%{0~9}%{0~9}
y %{1~9}%{2~9}-%{0~9}%{0~9} >= %{^}%{1~^-1}-%{0~9}%{0~9}

## Double/single digit tests -- opposite string sort order
n %{1~8}%{0~9}_%{0~9}%{0~9} <  %{^+1~9}_%{0~9}%{0~9}
n %{1~8}%{0~9}_%{0~9}%{0~9} <= %{^+1~9}_%{0~9}%{0~9}
y %{1~8}%{0~9}_%{0~9}%{0~9} >  %{^+1~9}_%{0~9}%{0~9}
y %{1~8}%{0~9}_%{0~9}%{0~9} >= %{^+1~9}_%{0~9}%{0~9}
n %{2~9}%{0~9}_%{0~9}%{0~9} <  %{1~^-1}_%{0~9}%{0~9}
n %{2~9}%{0~9}_%{0~9}%{0~9} <= %{1~^-1}_%{0~9}%{0~9}
y %{2~9}%{0~9}_%{0~9}%{0~9} >  %{1~^-1}_%{0~9}%{0~9}
y %{2~9}%{0~9}_%{0~9}%{0~9} >= %{1~^-1}_%{0~9}%{0~9}
y %{1~8}_%{0~9}%{0~9} <  %{^+1~9}%{0~9}_%{0~9}%{0~9}
y %{1~8}_%{0~9}%{0~9} <= %{^+1~9}%{0~9}_%{0~9}%{0~9}
n %{1~8}_%{0~9}%{0~9} >  %{^+1~9}%{0~9}_%{0~9}%{0~9}
n %{1~8}_%{0~9}%{0~9} >= %{^+1~9}%{0~9}_%{0~9}%{0~9}
y %{2~9}_%{0~9}%{0~9} <  %{1~^-1}%{0~9}_%{0~9}%{0~9}
y %{2~9}_%{0~9}%{0~9} <= %{1~^-1}%{0~9}_%{0~9}%{0~9}
n %{2~9}_%{0~9}%{0~9} >  %{1~^-1}%{0~9}_%{0~9}%{0~9}
n %{2~9}_%{0~9}%{0~9} >= %{1~^-1}%{0~9}_%{0~9}%{0~9}

## Two version levels: test the minor version differences
## Double digit tests -- first digits differ
y %{0~9}%{0~9}-%{1~8}%{0~9} <  %{^}%{^}-%{^+1~9}%{0~9}
y %{0~9}%{0~9}-%{1~8}%{0~9} <= %{^}%{^}-%{^+1~9}%{0~9}
n %{0~9}%{0~9}-%{1~8}%{0~9} >  %{^}%{^}-%{^+1~9}%{0~9}
n %{0~9}%{0~9}-%{1~8}%{0~9} >= %{^}%{^}-%{^+1~9}%{0~9}
n %{0~9}%{0~9}-%{2~9}%{0~9} <  %{^}%{^}-%{1~^-1}%{0~9}
n %{0~9}%{0~9}-%{2~9}%{0~9} <= %{^}%{^}-%{1~^-1}%{0~9}
y %{0~9}%{0~9}-%{2~9}%{0~9} >  %{^}%{^}-%{1~^-1}%{0~9}
y %{0~9}%{0~9}-%{2~9}%{0~9} >= %{^}%{^}-%{1~^-1}%{0~9}

## Double digit tests -- first digits the same
y %{0~9}%{0~9}_%{1~9}%{1~8} <  %{^}%{^}_%{^}%{^+1~9}
y %{0~9}%{0~9}_%{1~9}%{1~8} <= %{^}%{^}_%{^}%{^+1~9}
n %{0~9}%{0~9}_%{1~9}%{1~8} >  %{^}%{^}_%{^}%{^+1~9}
n %{0~9}%{0~9}_%{1~9}%{1~8} >= %{^}%{^}_%{^}%{^+1~9}
n %{0~9}%{0~9}_%{1~9}%{2~9} <  %{^}%{^}_%{^}%{1~^-1}
n %{0~9}%{0~9}_%{1~9}%{2~9} <= %{^}%{^}_%{^}%{1~^-1}
y %{0~9}%{0~9}_%{1~9}%{2~9} >  %{^}%{^}_%{^}%{1~^-1}
y %{0~9}%{0~9}_%{1~9}%{2~9} >= %{^}%{^}_%{^}%{1~^-1}

## Double/single digit tests -- opposite string sort order
n %{0~9}%{0~9}.%{1~8}%{0~9} <  %{^}%{^}.%{^+1~9}
n %{0~9}%{0~9}.%{1~8}%{0~9} <= %{^}%{^}.%{^+1~9}
y %{0~9}%{0~9}.%{1~8}%{0~9} >  %{^}%{^}.%{^+1~9}
y %{0~9}%{0~9}.%{1~8}%{0~9} >= %{^}%{^}.%{^+1~9}
n %{0~9}%{0~9}.%{2~9}%{0~9} <  %{^}%{^}.%{1~^-1}
n %{0~9}%{0~9}.%{2~9}%{0~9} <= %{^}%{^}.%{1~^-1}
y %{0~9}%{0~9}.%{2~9}%{0~9} >  %{^}%{^}.%{1~^-1}
y %{0~9}%{0~9}.%{2~9}%{0~9} >= %{^}%{^}.%{1~^-1}
y %{0~9}%{0~9}.%{1~8} <  %{^}%{^}.%{^+1~9}%{0~9}
y %{0~9}%{0~9}.%{1~8} <= %{^}%{^}.%{^+1~9}%{0~9}
n %{0~9}%{0~9}.%{1~8} >  %{^}%{^}.%{^+1~9}%{0~9}
n %{0~9}%{0~9}.%{1~8} >= %{^}%{^}.%{^+1~9}%{0~9}
y %{0~9}%{0~9}.%{2~9} <  %{^}%{^}.%{1~^-1}%{0~9}
y %{0~9}%{0~9}.%{2~9} <= %{^}%{^}.%{1~^-1}%{0~9}
n %{0~9}%{0~9}.%{2~9} >  %{^}%{^}.%{1~^-1}%{0~9}
n %{0~9}%{0~9}.%{2~9} >= %{^}%{^}.%{1~^-1}%{0~9}

## Double digit equality, two version levels
n %{1~9}%{0~9}.%{1~9}%{1~9} <  %{^}%{^}.%{^}%{^}
y %{1~9}%{0~9}.%{1~9}%{1~9} <= %{^}%{^}.%{^}%{^}
n %{1~9}%{0~9}.%{1~9}%{1~9} >  %{^}%{^}.%{^}%{^}
y %{1~9}%{0~9}.%{1~9}%{1~9} >= %{^}%{^}.%{^}%{^}

## Three version levels: test the least significant version
## Double digit tests -- first digits differ
y %{0~9}%{0~9}-%{0~9}%{0~9}-%{1~8}%{0~9} <  %{^}%{^}-%{^}%{^}-%{^+1~9}%{0~9}
y %{0~9}%{0~9}-%{0~9}%{0~9}-%{1~8}%{0~9} <= %{^}%{^}-%{^}%{^}-%{^+1~9}%{0~9}
n %{0~9}%{0~9}-%{0~9}%{0~9}-%{1~8}%{0~9} >  %{^}%{^}-%{^}%{^}-%{^+1~9}%{0~9}
n %{0~9}%{0~9}-%{0~9}%{0~9}-%{1~8}%{0~9} >= %{^}%{^}-%{^}%{^}-%{^+1~9}%{0~9}
n %{0~9}%{0~9}-%{0~9}%{0~9}-%{2~9}%{0~9} <  %{^}%{^}-%{^}%{^}-%{1~^-1}%{0~9}
n %{0~9}%{0~9}-%{0~9}%{0~9}-%{2~9}%{0~9} <= %{^}%{^}-%{^}%{^}-%{1~^-1}%{0~9}
y %{0~9}%{0~9}-%{0~9}%{0~9}-%{2~9}%{0~9} >  %{^}%{^}-%{^}%{^}-%{1~^-1}%{0~9}
y %{0~9}%{0~9}-%{0~9}%{0~9}-%{2~9}%{0~9} >= %{^}%{^}-%{^}%{^}-%{1~^-1}%{0~9}

## Double digit tests -- first digits the same
y %{0~9}%{0~9}_%{0~9}%{0~9}_%{1~9}%{1~8} <  %{^}%{^}_%{^}%{^}_%{^}%{^+1~9}
y %{0~9}%{0~9}_%{0~9}%{0~9}_%{1~9}%{1~8} <= %{^}%{^}_%{^}%{^}_%{^}%{^+1~9}
n %{0~9}%{0~9}_%{0~9}%{0~9}_%{1~9}%{1~8} >  %{^}%{^}_%{^}%{^}_%{^}%{^+1~9}
n %{0~9}%{0~9}_%{0~9}%{0~9}_%{1~9}%{1~8} >= %{^}%{^}_%{^}%{^}_%{^}%{^+1~9}
n %{0~9}%{0~9}_%{0~9}%{0~9}_%{1~9}%{2~9} <  %{^}%{^}_%{^}%{^}_%{^}%{1~^-1}
n %{0~9}%{0~9}_%{0~9}%{0~9}_%{1~9}%{2~9} <= %{^}%{^}_%{^}%{^}_%{^}%{1~^-1}
y %{0~9}%{0~9}_%{0~9}%{0~9}_%{1~9}%{2~9} >  %{^}%{^}_%{^}%{^}_%{^}%{1~^-1}
y %{0~9}%{0~9}_%{0~9}%{0~9}_%{1~9}%{2~9} >= %{^}%{^}_%{^}%{^}_%{^}%{1~^-1}

## Double/single digit tests -- opposite string sort order
n %{0~9}%{0~9}.%{0~9}%{0~9}.%{1~8}%{0~9} <  %{^}%{^}.%{^}%{^}.%{^+1~9}
n %{0~9}%{0~9}.%{0~9}%{0~9}.%{1~8}%{0~9} <= %{^}%{^}.%{^}%{^}.%{^+1~9}
y %{0~9}%{0~9}.%{0~9}%{0~9}.%{1~8}%{0~9} >  %{^}%{^}.%{^}%{^}.%{^+1~9}
y %{0~9}%{0~9}.%{0~9}%{0~9}.%{1~8}%{0~9} >= %{^}%{^}.%{^}%{^}.%{^+1~9}
n %{0~9}%{0~9}.%{0~9}%{0~9}.%{2~9}%{0~9} <  %{^}%{^}.%{^}%{^}.%{1~^-1}
n %{0~9}%{0~9}.%{0~9}%{0~9}.%{2~9}%{0~9} <= %{^}%{^}.%{^}%{^}.%{1~^-1}
y %{0~9}%{0~9}.%{0~9}%{0~9}.%{2~9}%{0~9} >  %{^}%{^}.%{^}%{^}.%{1~^-1}
y %{0~9}%{0~9}.%{0~9}%{0~9}.%{2~9}%{0~9} >= %{^}%{^}.%{^}%{^}.%{1~^-1}
y %{0~9}%{0~9}.%{0~9}%{0~9}.%{1~8} <  %{^}%{^}.%{^}%{^}.%{^+1~9}%{0~9}
y %{0~9}%{0~9}.%{0~9}%{0~9}.%{1~8} <= %{^}%{^}.%{^}%{^}.%{^+1~9}%{0~9}
n %{0~9}%{0~9}.%{0~9}%{0~9}.%{1~8} >  %{^}%{^}.%{^}%{^}.%{^+1~9}%{0~9}
n %{0~9}%{0~9}.%{0~9}%{0~9}.%{1~8} >= %{^}%{^}.%{^}%{^}.%{^+1~9}%{0~9}
y %{0~9}%{0~9}.%{0~9}%{0~9}.%{2~9} <  %{^}%{^}.%{^}%{^}.%{1~^-1}%{0~9}
y %{0~9}%{0~9}.%{0~9}%{0~9}.%{2~9} <= %{^}%{^}.%{^}%{^}.%{1~^-1}%{0~9}
n %{0~9}%{0~9}.%{0~9}%{0~9}.%{2~9} >  %{^}%{^}.%{^}%{^}.%{1~^-1}%{0~9}
n %{0~9}%{0~9}.%{0~9}%{0~9}.%{2~9} >= %{^}%{^}.%{^}%{^}.%{1~^-1}%{0~9}

## Test multiple levels against a lesser number
n %{0~9}-%{1~9} <  %{^}
n %{0~9}-%{1~9} <= %{^}
y %{0~9}-%{1~9} >  %{^}
y %{0~9}-%{1~9} >= %{^}
y %{0~9} <  %{^}.%{1~9}
y %{0~9} <= %{^}.%{1~9}
n %{0~9} >  %{^}.%{1~9}
n %{0~9} >= %{^}.%{1~9}
## Test against even a zero version (which still counts as greater)
n %{0~9}-0 <  %{^}
n %{0~9}-0 <= %{^}
y %{0~9}-0 >  %{^}
y %{0~9}-0 >= %{^}
y %{0~9} <  %{^}.0
y %{0~9} <= %{^}.0
n %{0~9} >  %{^}.0
n %{0~9} >= %{^}.0

## Test with additional characters
y %{0~9} < %{^}p
n %{0~9} >= %{^}x
n %{0~9}a <= %{^}
y %{0~9}b >= %{^}

## Test with letters as separators
n %{0~9}%{0~9}a%{0~9}%{0~9}a%{1~8}%{0~9} <  %{^}%{^}a%{^}%{^}a%{^+1~9}
n %{0~9}%{0~9}b%{0~9}%{0~9}b%{1~8}%{0~9} <= %{^}%{^}b%{^}%{^}b%{^+1~9}
y %{0~9}%{0~9}c%{0~9}%{0~9}c%{1~8}%{0~9} >  %{^}%{^}c%{^}%{^}c%{^+1~9}
y %{0~9}%{0~9}d%{0~9}%{0~9}d%{1~8}%{0~9} >= %{^}%{^}d%{^}%{^}d%{^+1~9}
n %{0~9}%{0~9}e%{0~9}%{0~9}e%{2~9}%{0~9} <  %{^}%{^}e%{^}%{^}e%{1~^-1}
n %{0~9}%{0~9}f%{0~9}%{0~9}f%{2~9}%{0~9} <= %{^}%{^}f%{^}%{^}f%{1~^-1}
y %{0~9}%{0~9}g%{0~9}%{0~9}g%{2~9}%{0~9} >  %{^}%{^}g%{^}%{^}g%{1~^-1}
y %{0~9}%{0~9}h%{0~9}%{0~9}h%{2~9}%{0~9} >= %{^}%{^}h%{^}%{^}h%{1~^-1}
y %{0~9}%{0~9}i%{0~9}%{0~9}i%{1~8} <  %{^}%{^}i%{^}%{^}i%{^+1~9}%{0~9}
y %{0~9}%{0~9}j%{0~9}%{0~9}j%{1~8} <= %{^}%{^}j%{^}%{^}j%{^+1~9}%{0~9}
n %{0~9}%{0~9}k%{0~9}%{0~9}k%{1~8} >  %{^}%{^}k%{^}%{^}k%{^+1~9}%{0~9}
n %{0~9}%{0~9}l%{0~9}%{0~9}l%{1~8} >= %{^}%{^}l%{^}%{^}l%{^+1~9}%{0~9}
y %{0~9}%{0~9}m%{0~9}%{0~9}m%{2~9} <  %{^}%{^}m%{^}%{^}m%{1~^-1}%{0~9}
y %{0~9}%{0~9}n%{0~9}%{0~9}n%{2~9} <= %{^}%{^}n%{^}%{^}n%{1~^-1}%{0~9}
n %{0~9}%{0~9}o%{0~9}%{0~9}o%{2~9} >  %{^}%{^}o%{^}%{^}o%{1~^-1}%{0~9}
n %{0~9}%{0~9}p%{0~9}%{0~9}p%{2~9} >= %{^}%{^}p%{^}%{^}p%{1~^-1}%{0~9}

## Test with letters and symbols as separators
n %{0~9}%{0~9}.%{0~9}%{0~9}a%{1~8}%{0~9} <  %{^}%{^}.%{^}%{^}a%{^+1~9}
n %{0~9}%{0~9}.%{0~9}%{0~9}b%{1~8}%{0~9} <= %{^}%{^}.%{^}%{^}b%{^+1~9}
y %{0~9}%{0~9}.%{0~9}%{0~9}c%{1~8}%{0~9} >  %{^}%{^}.%{^}%{^}c%{^+1~9}
